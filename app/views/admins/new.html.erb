<h1>Register</h1>
<%= simple_form_for @admin do |f| %>
<%= f.input :first_name %>
<%= f.input :last_name %>
<%= f.input :email %>
<%= f.input :pass %>
<%= f.input :pass_confirmation %>
<%= f.button :submit %>
<% end %>

<h1>Login</h1>
<%= simple_form_for @admin do |f| %>
<%= f.input :email %>
<%= f.input :pass %>
<%= f.button :submit %>
<% end %>